  "description": "Smart collecting your ideas,rank them,mark them as completed,and insert pros,cons,risks and rewards",
  "main": "index.js",
  "scripts": {
    "test": "eslint --fix **/src/",
    "precommit": "npm test",
    "prepush": "npm test",
    "heroku:add": "heroku git:remote -a",
